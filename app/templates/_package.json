{
  "name": "<%= repoName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= repoName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && <% if (babel) { %>npm run build && <% } %>jest"<% if (babel) { %>,
    "build": "babel lib --out-dir=dist --optional=runtime"<% } %>
  },
  "files": [<% if (babel) { %>
    "dist"<% if (cli) { %>,<% } %>
  <% } else { %>
    "index.js"<% if (cli) { %>,<% } %>
  <% } %><% if (cli) { %>
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    ""
  ],
  "dependencies": {<% if (cli && babel) { %>
    "meow": "*",
    "babel-runtime": "*"
    <% } else if (cli) { %>
    "meow": "*"
    <% } else if (babel) { %>
    "babel-runtime": "*"
    <% } else if (updateNotifier) { %>
    "update-notifier": "*"
    <% } %>
  },
  "devDependencies": {
    "jest": "*",
    "xo": "*"<% if (babel) { %>,
    "babel": "*"<% } %> <% if (codecov) { %>,
    "coveralls": "*"<% } %>
  }<% if (babel) { %>,
  "xo": {
    "esnext": true,
    "ignores": [
      "dist/**"
  ]}<% } %><% if (codecov) { %>,
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js",
      "!/node_modules/**"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  }
  <% } %>
}
