{
	"name": "generator-nmx",
	"version": "1.1.3",
	"description": "Scaffold out a node module.",
	"license": "MIT",
	"repository": "radiovisual/generator-nmx",
	"author": {
		"name": "Michael Wuergler",
		"email": "wuergler@gmail.com",
		"url": "https://github.com/radiovisual"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"test": "xo && jest --runTestsByPath test.js",
		"lint:fix": "xo --fix"
	},
	"files": [
		"app"
	],
	"keywords": [
		"yeoman-generator",
		"plugin",
		"boilerplate",
		"template",
		"scaffold",
		"node",
		"module",
		"init",
		"extras",
		"coverage",
		"git",
		"github",
		"babel",
		"jest",
		"es6",
		"codecov",
		"update",
		"notify"
	],
	"dependencies": {
		"normalize-url": "^1.2.0",
		"superb": "^1.3.0",
		"underscore.string": "^3.0.3",
		"yeoman-generator": "^1.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.24.4",
		"@babel/preset-env": "^7.24.4",
		"babel-jest": "^29.7.0",
		"jest": "^29.7.0",
		"pify": "^3.0.0",
		"xo": "^0.58.0",
		"yeoman-assert": "^3.1.1",
		"yeoman-test": "^6.3.0"
	},
	"xo": {
		"esnext": true,
		"ignores": [
			"app/templates/**"
		],
		"rules": {
			"unicorn/prefer-module": "off"
		}
	}
}
